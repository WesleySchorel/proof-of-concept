<section class="section">
 <section class="section__header">
   <h2>Aanvraag</h2>
   <h3><%= data.request.firstName %> <%= data.request.lastName %>, <%= data.request.organisationName %></h3>
 </section>

 <div class="section__content">
  <form class="form" method="post">
    <fieldset class="set">
      <div class="field">
        <label class="field__label" for="firstName">Voornaam</label>
        <input
          class="field__input"
          type="text"
          name="firstName"
          id="firstName"
          value="<%= data.request.firstName %>"
          required />
      </div>
      <div class="field">
        <label class="field__label" for="lastName">Achternaam</label>
        <input
          class="field__input"
          type="text"
          name="lastName"
          id="lastName"
          value="<%= data.request.lastName %>"
          required />
      </div>
    </fieldset>
    <div class="field">
      <label class="field__label" for="emailAddress">Email</label>
      <input
        class="field__input"
        type="email"
        name="emailAddress"
        id="emailAddress"
        value="<%= data.request.emailAddress %>"
        required />
    </div>
    <div class="field">
      <label class="field__label" for="organisationName">Bedrijf</label>
      <input
        class="field__input"
        type="text"
        name="organisationName"
        id="organisationName"
        value="<%= data.request.organisationName %>"
        />
    </div>
    <div class="field">
      <label class="field__label" for="phoneNumber">Telefoonnummer</label>
      <input
        class="field__input"
        type="text"
        name="phoneNumber"
        id="phoneNumber"
        value="<%= data.request.phoneNumber %>"
        required />
    </div>


 <div class="request-buttons">
      <button class="approve-button" type="button">
          <h4>Goedkeuren</h4>
      </button>

      <button class="disapprove-button" type="button">
         <h4>Afkeuren</h4>
    </button>
 </div>
</article>

